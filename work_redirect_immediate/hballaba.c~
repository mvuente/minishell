#include "minishell.h"

void    ft_creat_env(char *env[], t_env	**bufenv)
{
   int t = -1;

    while (env[++t])
        ft_lstadd_back_env(bufenv, ft_lstnew_env(env[t]));
	return ;
}


void ft_init_all(t_all *all, char **env, t_env *bufenv)
{
    
    all->fd_0 = dup(0);
    all->fd_1 = dup(1);
    ft_creat_env(env, &bufenv);
    all->myenv = *bufenv;
	return ;
}